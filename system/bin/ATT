
#!/system/bin/sh
#All-in-One Project
#Scripted - Asiier & Senthil360
#Tweaks - @Senthil360 @Mostafa Wael @Patalao @Asiier
BB=/system/etc/AKT/busybox
divider="  =============================  "
echo ""
echo $divider
echo -e "\e[01;36m   - All-in-One Project - \e[00;37m"
echo -e "\e[01;36m - Advanced路Kernel路Tweaks - \e[00;37m"
echo -e "\e[01;36m          - A路K路T -   \e[00;37m"
echo -e "\e[01;36m        ~ Stable 1.3 ~   \e[00;37m"
echo $divider
sleep 1
echo ""
echo -e "\e[01;36m *** By Asiier & Senthil360 *** \e[00;37m"
echo ""
sleep 2
clear
function Battery
{
while true; do
echo -e "\e[01;91m   UP  Less Battery / More Performance \e[00;37m"
echo -e "\e[01;33m   | \e[00;37m"
echo -e "\e[01;93m   | \e[00;37m"
echo -e "\e[01;92m DOWN  More Battery / Less Performance \e[00;37m"
echo ""
sleep 3
echo -e "\e[02;91m 1.) Project Zhana Battery by @Asiier \e[00;37m"
echo -e "\e[01;31m 2.) Nameless Battery by @Senthil360  \e[00;37m"
echo -e "\e[02;33m 3.) FairPark by @Asiier \e[00;37m"
echo -e "\e[01;93m 4.) Fusion Conservative by @patalao \e[00;37m"
echo -e "\e[01;93m 5.) Project X.A.N.A Battery by @Asiier \e[00;37m"
echo -e "\e[01;92m 6.) Project X.A.N.A Battery Extreme by @Asiier \e[00;37m"
echo -e "\e[01;90m 7.) Additional tweaks... \e[00;37m"
echo -e "\e[01;94m 8.) Main menu \e[00;37m"
echo ""
echo $divider
echo ""
echo -n "Please select a profile:"
read option
echo ""
case $option in
1 ) #Project_Zhana_Battery
$BB mount -o remount,rw /system 2>/dev/null
mount -o remount,rw /system 2>/dev/null
if [ -e "/system/su.d/99AKT" ]; then
cat /system/etc/AKT/PZ_battery > /system/su.d/99AKT
chmod 700 /system/su.d/99AKT
fi
if [ -e "/system/etc/init.d/99AKT" ]; then
cat /system/etc/AKT/PZ_battery > /system/etc/init.d/99AKT
chmod 777 /system/etc/init.d/99AKT
fi
chmod 777 /system/etc/AKT/PZ_battery
clear
sh /system/etc/AKT/PZ_battery
clear
echo " Installation finished... "
echo " For additional information and questions, please visit our XDA thread"
sleep 5; main_menu;
exit;;
2 ) #Nameless_Battery 
$BB mount -o remount,rw /system 2>/dev/null
mount -o remount,rw /system 2>/dev/null
if [ -e "/system/su.d/99AKT" ]; then
cat /system/etc/AKT/NLB_battery > /system/su.d/99AKT
chmod 700 /system/su.d/99AKT
fi
if [ -e "/system/etc/init.d/99AKT" ]; then
cat /system/etc/AKT/NLB_battery > /system/etc/init.d/99AKT
chmod 777 /system/etc/init.d/99AKT
fi
chmod 777 /system/etc/AKT/NLB_battery
clear
sh /system/etc/AKT/NLB_battery
clear
echo " Installation finished... "
echo " For additional information and questions, please visit our XDA thread"
sleep 5; main_menu;
exit;;
3 ) #FairPark
$BB mount -o remount,rw /system 2>/dev/null
mount -o remount,rw /system 2>/dev/null
if [ -e "/system/su.d/99AKT" ]; then
cat /system/etc/AKT/FairPark > /system/su.d/99AKT
chmod 700 /system/su.d/99AKT
fi
if [ -e "/system/etc/init.d/99AKT" ]; then
cat /system/etc/AKT/FairPark > /system/etc/init.d/99AKT
chmod 777 /system/etc/init.d/99AKT
fi
chmod 777 /system/etc/AKT/FairPark
clear
sh /system/etc/AKT/FairPark
clear
echo " Installation finished... "
echo " For additional information and questions, please visit our XDA thread"
sleep 5; main_menu;
exit;;
4 ) #Fusion Conservative
$BB mount -o remount,rw /system 2>/dev/null
mount -o remount,rw /system 2>/dev/null
if [ -e "/system/su.d/99AKT" ]; then
cat /system/etc/AKT/FusionC > /system/su.d/99AKT
chmod 700 /system/su.d/99AKT
fi
if [ -e "/system/etc/init.d/99AKT" ]; then
cat /system/etc/AKT/FusionC > /system/etc/init.d/99AKT
chmod 777 /system/etc/init.d/99AKT
fi
chmod 777 /system/etc/AKT/FusionC
clear
sh /system/etc/AKT/FusionC
clear
echo " Installation finished... "
echo " For additional information and questions, please visit our XDA thread"
sleep 5; main_menu;
exit;;
5 ) #Project_XANA_Battery
$BB mount -o remount,rw /system 2>/dev/null
mount -o remount,rw /system 2>/dev/null
if [ -e "/system/su.d/99AKT" ]; then
cat /system/etc/AKT/PX_battery > /system/su.d/99AKT
chmod 700 /system/su.d/99AKT
fi
if [ -e "/system/etc/init.d/99AKT" ]; then
cat /system/etc/AKT/PX_battery > /system/etc/init.d/99AKT
chmod 777 /system/etc/init.d/99AKT
fi
chmod 777 /system/etc/AKT/PX_battery
clear
sh /system/etc/AKT/PX_battery
clear
echo " Installation finished... "
echo " For additional information and questions, please visit our XDA thread"
sleep 5; main_menu;
exit;;
6 ) #Project_XANA_Extreme
$BB mount -o remount,rw /system 2>/dev/null
mount -o remount,rw /system 2>/dev/null
if [ -e "/system/su.d/99AKT" ]; then
cat /system/etc/AKT/PX_battery_extreme > /system/su.d/99AKT
chmod 700 /system/su.d/99AKT
fi
if [ -e "/system/etc/init.d/99AKT" ]; then
cat /system/etc/AKT/PX_battery_extreme > /system/etc/init.d/99AKT
chmod 777 /system/etc/init.d/99AKT
fi
chmod 777 /system/etc/AKT/PX_battery_extreme
clear
sh /system/etc/AKT/PX_battery_extreme
clear
echo " Installation finished... "
echo " For additional information and questions, please visit our XDA thread"
sleep 5; main_menu;
exit;;
7 ) echo "Coming soon"
echo " You tell me what to add here"
sleep 2;
clear; main_menu;
exit;;
8 ) clear; main_menu;;
* ) clear; echo " Please, enter a valid option..."; 
sleep 2;
clear;;
esac
# }
 done
}
