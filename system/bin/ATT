function Remove
{
echo ""
chmod 000 /system/etc/init.d/99AKT
chmod 000 /system/su.d/99AKT
rm -rf /system/su.d/99AKT
rm -rf /system/etc/init.d/99AKT
rm -rf /system/etc/AKT
rm -rf /system/bin/AKT
echo -e "\e[01;31m Permission resetted... \e[00;37m"
echo -e "\e[01;31m Files removed from /sytem ... \e[00;37m"
echo -e "\e[01;31m Please, reboot the device for fully removing the tweaks... \e[00;37m"
sleep 3
$BB mount -o remount,ro /system 2>/dev/null
mount -o remount,ro /system 2>/dev/null
clear;
exit
 }
function Disable
{
echo ""
$BB mount -o remount,rw /system 2>/dev/null
mount -o remount,rw /system 2>/dev/null
cat /system/etc/AKT/DisableInitd > /system/etc/init.d/99AKT
cat /system/etc/AKT/DisableInitd > /system/su.d/99AKT
chmod 644 /system/etc/init.d/99AKT
chmod 644 /system/su.d/99AKT
echo -e "\e[01;31m Permission resetted... \e[00;37m"
echo -e "\e[01;31m File overwritten ... \e[00;37m"
echo -e "\e[01;31m It's recommended to perform a reboot for disabling the tweaks \e[00;37m"
sleep 2
echo -e "\e[02;39m Restoring stock values ... \e[00;37m"
sleep 3
chmod 777 /system/etc/AKT/Stock
clear
sh /system/etc/AKT/Stock
clear;
 }
function main_menu
{
while true; do
clear
echo ""
echo $divider
echo -e "\e[01;36m - Advanced路Kernel路Tweaks - \e[00;37m"
echo -e "\e[01;36m          - A路K路T -   \e[00;37m"
echo $divider
echo ""
echo -e "\e[01;32m  1. Battery \e[00;37m"
echo ""
echo -e "\e[01;34m  2. Balanced \e[00;37m"
echo ""
echo -e "\e[01;31m  3. Performance \e[00;37m"
echo ""
echo $divider
echo ""
echo -e "\e[01;37m 4. Disable Tweaks \e[00;37m"
echo ""
echo -e "\e[01;37m 5. Remove A路K路T! \e[00;37m"
echo ""
echo -e "\e[00;36m 6. Exit \e[00;37m"
echo ""
if [ -e /data/system/current_profile ]; then
 echo $divider
 echo ""
 string3=$($BB cat /data/system/current_profile)
 echo -e " Current Profile:  \e[01;94m'\e[04;94m$string3\e[00;37m\e[01;94m'\e[00;37m"
 echo ""
 echo $divider
fi
echo -n "Enter choice: "
read option
echo ""
case $option in
	1 ) clear; Battery ;;
	2 ) clear; Balanced ;;
	3 ) clear; Performance ;;
	4 ) clear; Disable ;;
	5 ) clear; Remove ;;
	6 ) clear; exit ; $BB mount -o remount,ro /system 2>/dev/null ; mount -o remount,ro /system 2>/dev/null ;; 
	* ) echo " Please, enter a valid option..."
	sleep 2
	clear;;
esac
done
 }
 
while true; do
main_menu
done
